{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "replicaCount": {
            "type": "integer",
            "examples": [
                1
            ]
        },
        "image": {
            "type": "string",
            "examples": [
                "quay.io/telescope/dashboard-react-app:latest"
            ]
        },
        "imagePullPolicy": {
            "type": "string",
            "examples": [
                "Always"
            ]
        },
        "backendUrl": {
            "type": "string",
            "examples": [
                ""
            ]
        },
        "nodeBuildDir": {
            "type": "string",
            "examples": [
                "/opt/app-root/src/build"
            ]
        },
        "service": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "examples": [
                        "ClusterIP"
                    ]
                },
                "port": {
                    "type": "integer",
                    "examples": [
                        80
                    ]
                },
                "targetport": {
                    "type": "integer",
                    "examples": [
                        3000
                    ]
                }
            },
            "examples": [{
                "type": "ClusterIP",
                "port": 80,
                "targetport": 3000
            }]
        },
        "probe": {
            "type": "object",
            "properties": {
                "readypath": {
                    "type": "string",
                    "examples": [
                        "/"
