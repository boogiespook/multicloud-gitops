        },
        "vaultSecret": {
            "type": "string",
            "examples": [
                "vault-secret"
            ]
        },
        "vaultKey": {
            "type": "string",
            "examples": [
                "token"
            ]
        },
        "vault": {
            "type": "object",
            "properties": {
                "registry2": {
                    "type": "object",
