                    "examples": [{
                        "host": {
                            "key": "secret/registry2",
                            "property": "host"
                        },
                        "user": {
                            "key": "secret/registry2",
                            "property": "user"
                        },
                        "password": {
                            "key": "secret/registry2",
                            "property": "password"
                        }
                    }]
                }
            },
            "examples": [{
                "registry2": {
                    "host": {
                        "key": "secret/registry2",
                        "property": "host"
                    },
                    "user": {
                        "key": "secret/registry2",
                        "property": "user"
                    },
                    "password": {
                        "key": "secret/registry2",
                        "property": "password"
                    }
                }
            }]
        }
    },
    "examples": [{
        "replicaCount": 1,
        "image": "quay.io/telescope/dashboard-react-app:latest",
        "imagePullPolicy": "Always",
        "backendUrl": "",
        "nodeBuildDir": "/opt/app-root/src/build",
        "service": {
            "type": "ClusterIP",
            "port": 80,
            "targetport": 3000
        },
        "probe": {
            "readypath": "/",
            "livepath": "/"
        },
        "route": {
            "enabled": true,
            "annotations": {},
            "path": "/",
            "hostname": ""
        },
        "resources": null,
        "nodeSelector": {},
        "tolerations": [],
        "affinity": {},
        "env": [],
        "pullSecret": {
            "enabled": false,
            "secretName": "pull-secret",
            "secretKey": ".dockerconfigjson"
        },
        "vaultStorage": "vault-backend",
        "vaultSecret": "vault-secret",
        "vaultKey": "token",
        "vault": {
            "registry2": {
                "host": {
                    "key": "secret/registry2",
                    "property": "host"
                },
                "user": {
                    "key": "secret/registry2",
                    "property": "user"
                },
                "password": {
                    "key": "secret/registry2",
                    "property": "password"
                }
            }
        }
    }]
